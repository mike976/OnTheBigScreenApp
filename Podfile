# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'TheBigScreenDb' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TheBigScreenDb

    # Networking APIS
    pod 'Alamofire', '~> 5.1'
    pod 'AlamofireImage', '~> 4.1'
    pod 'SwiftyJSON'
    
    # Create UI by code API
    pod 'PureLayout'
    pod 'STRatingControl'



  target 'TheBigScreenDbTests' do
    inherit! :search_paths
    # Pods for testing


# Networking APIS
    pod 'Alamofire', '~> 5.1'
    pod 'AlamofireImage', '~> 4.1'
    pod 'SwiftyJSON'
    
    # Create UI by code API
    pod 'PureLayout'
    pod 'STRatingControl'
        


  end

  target 'TheBigScreenDbUITests' do
    # Pods for testing


# Networking APIS
    pod 'Alamofire', '~> 5.1'
    pod 'AlamofireImage', '~> 4.1'
    pod 'SwiftyJSON'

    
    # Create UI by code API
    pod 'PureLayout'
    pod 'STRatingControl'


  end

end
